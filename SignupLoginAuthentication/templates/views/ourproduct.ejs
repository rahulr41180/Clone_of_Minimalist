async function AddToCart(product) 
{

    var product = JSON.parse(product);
    var productId = product._id;
    fetch("/cart/allproduct")
    .then((res) => 
    {
        return res.json();
    })
    .then((res) => 
    {
        var result = findproduct(res, productId);
        if (!result) // --> false
        {
          post_product(productId);
        }
        else // --> true
        {
          patch_product(result);
        }
    });
}
